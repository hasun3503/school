(3)[for(딕셔너리)]
for our_pots in {"hs":10, "ab":30,"yu":"none"}:
    print(our_pots)

(4)[for(딕셔너리)]
our_pots={"hs":10, "ab":30,"yu":"none"}
for key in our_pots:
    print(our_pots[key])
    
(5)[for(딕셔너리)]
our_pots={"hs":10, "ab":30,"yu":"none"}
for key in our_pots:
    print(key,"의 집에는",our_pots[key],"개의 냄비가 있다.")
    
(6)[for(딕셔너리+포멧)]
our_pots={"hs":10, "ab":30,"yu":"none"}
for key in our_pots:
    print("{}의 집에는 {}개의 냄비가 있다.".format(key,our_pots[key]))

