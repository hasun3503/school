(8)[for]
for i in range(2,9+1):
    for j in range(1,9+1):
        print("{}*{} = {}".format(i,j,i*j))
        
(9)[for]
i = 2
while i <= 9:
    j = 1
    i+=1
    while j<=9:
        print("{}*{} = {}".format(i,j,i*j))
        j+=1



