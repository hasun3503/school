#include <stdio.h>

int main()
{
    int num =0;
    printf("정보과목의 점수를 입력하시오.\n");
    scanf("%d", &num);
    
    switch(num) {
    case 0:printf("F학점 입니다.");
        break;
    case 20:printf("E학점 입니다.");
        break;
    case 40:printf("D학점 입니다.");
        break;
    case 60:printf("C학점 입니다.");
        break;
    case 80:printf("B학점 입니다.");
        break;
    case 100:printf("A학점 입니다.");
        break;
  default:printf("잘못입력하셨습니다.");
}
    return 0;
}
